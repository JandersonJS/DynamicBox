<?xml version="1.0" encoding="UTF-8"?>
<theme>
  <formatVersion>7</formatVersion>
  <include>./animatedbackgroundsubset.xml</include>
  <view name="system">
    <video name="videobox" extra="static" ifSubset="randomVideosAudio:on">
      <default>./main/video/no-video.mp4</default>
      <path>{random}</path>
      <minSize>0.216 0.198</minSize>
      <origin>0.5 0.5</origin>
      <pos>0.8777 0.793</pos>
      <showVideoAtDelay>0</showVideoAtDelay>
      <efect>none</efect>
      <audio>true</audio>
      <zIndex>40</zIndex>
    </video>

    <video name="videobox" extra="static" ifSubset="randomVideosAudio:off">
      <default>./main/video/no-video.mp4</default>
      <path>{random}</path>
      <minSize>0.216 0.198</minSize>
      <origin>0.5 0.5</origin>
      <pos>0.8777 0.793</pos>
      <showVideoAtDelay>0</showVideoAtDelay>
      <efect>none</efect>
      <audio>false</audio>
      <zIndex>40</zIndex>
    </video>
  </view>
</theme>
